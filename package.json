{
  "name": "teneturaut",
  "description": "A simple, maximally extensible, dependency minimized framework for building modern Ethereum dApps",
  "keywords": [
    "qui",
    "react-hooks",
    "sed",
    "et",
    "javascript",
    "impedit",
    "web3",
    "context",
    "frontend",
    "qui",
    "quaerat",
    "walletconnect",
    "walletlink",
    "in",
    "laudantium",
    "portis",
    "et",
    "quis",
    "deserunt",
    "nemo"
  ],
  "author": "CaesarAltenwerth",
  "repository": {
    "type": "git",
    "url": "https://github.com/GaylordOsinski/teneturaut.git"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "publish:lerna": "lerna publish",
    "clean": "lerna clean --yes && lerna exec -- rimraf yarn.lock dist/ .rts2_cache_{cjs,esm}/",
    "build": "lerna run build",
    "start": "lerna run --parallel --no-bail start -- --noClean",
    "lint": "lerna run lint --parallel",
    "new-version": "lerna version --conventional-commits --yes",
    "publish": "lerna publish from-git",
    "test": "lerna run test --parallel"
  },
  "devDependencies": {
    "@types/jest": "^24.0.21",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "lerna": "^3.19.0",
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "rimraf": "^3.0.1",
    "tsdx": "^0.11.0"
  },
  "license": "GPL-3.0-or-later",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "bugs": {
    "url": "https://github.com/GaylordOsinski/teneturaut.git/issues"
  },
  "version": "1.0.0"
}